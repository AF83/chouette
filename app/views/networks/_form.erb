<%= semantic_form_for [@referential, @network] do |form| %>
  <%= form.inputs do %> 
    <%= form.input :name %> 
    <%= form.input :registration_number %>  
    <%= form.input :comment %>
    <%= form.input :version_date %> 
    <%= form.input :description %>              
    <%= form.input :source_name %>              
    <%= form.input :source_identifier %>              
  <% end %>

  <%= form.buttons do %>
    <%= form.commit_button %>
    <li><%= t('or') %></li>
    <li><%= link_to t('cancel'), :back %></li>
  <% end %>
<% end %>
