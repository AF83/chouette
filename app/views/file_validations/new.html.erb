      <%= form.input :projection_reference ,:input_html => { :value => 'WGS84' }, :wrapper_html => { :class => 'fl' } %>
  <% end %>

   <%= form.actions do %>
     <%= form.action :submit, :as => :button , :label => t( 'formtastic.validate' ) %>
     <%= form.action :cancel, :as => :link %>
   <% end %>
<% end %>
